{
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/v1/api/(.*)",
            "destination": "/api/(.*)"
        },
        {
            "source": "/v1/api/with-gin/(.*)",
            "destination": "/api/with-gin.go"
        },
        {
            "source": "/v1/api/user/(.*)",
            "destination": "/api/user.go"
        }
    ],
    "build": {
      "env": {
        "GO_BUILD_FLAGS": "-ldflags '-s -w'"
      }
    }
}